{
  "expo": {
    "name": "FlyDeals",
    "slug": "FlyDeals",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-custom.png",
    "scheme": "flydeals",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.raflyazizabdillah.flydeals"
    },
    "android": {
      "package": "com.raflyazizabdillah.flydeals",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon-custom.png",
        "backgroundColor": "#77B254"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "splash": {
      "image": "./assets/images/splash-icon-custom.png",
      "imageWidth": 200,
      "resizeMode": "contain",
      "backgroundColor": "#77B254"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon-custom.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#77B254"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "fd8ea5d7-23f2-45e4-845c-4b31b1e09f32"
      }
    }
  }
}
